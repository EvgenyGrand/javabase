package com.company;
public class Main {
    public static void main(String[] args) {
        for (int i = 10; i >0; i--) {
            System.out.println(i);
        }
        System.out.println();
        for (int j=0; j<10;j++){
            System.out.println(j);
        }
       for (int i=1;i<=9;i++){
           System.out.println();
           for(int j=1;j<=9;j++){
               System.out.println(i+"*"+j+"="+i*j);
           }
       }
    }
    }
